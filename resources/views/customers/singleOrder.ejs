<section class="status">
    <div class="container mx-auto">
        <div class="status-box mx-auto w-2/3">
            <div class="flex item-center justify-between mb-12">
                <h1 class="font-bold text-xl">Track Delivery Satus</h1>
                <h6 class="py-1 rounded-full bg-white px-4 text-green-600 text-xs"><%= order.id %> </h6>
                <input id="hiddenInput" type="hidden" value="<%= JSON.stringify(order) %> ">
            </div>
            <ul>
                <li class="status_line text-xl pb-16" data-status="order_placed"><span>Order Placed</span></li>
                <li class="status_line text-xl pb-16" data-status="confirmed"><span>Order Confirmation</span></li>
                <li class="status_line text-xl pb-16" data-status="prepared"><span>Order Preparation</span></li>
                <li class="status_line text-xl pb-16" data-status="delivered"><span>Out for delivery</span></li>
                <li class="status_line text-xl" data-status="completed"><span>Complete</span></li>
            </ul>
        </div>

    </div>

</section>